{
  "name": "crud_backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "CI=false && ttsc -p tsconfig.build.json",
    "start": "node dist/server.js",
    "seed": "ts-node prisma/seed.ts",
    "migrate-and-start": "npx prisma migrate deploy && npm start",
    "migrate-seed-and-start": "npx prisma migrate deploy && npx prisma db seed && npm start",
    "generate-migrate-seed-start": "npx prisma generate && npx prisma migrate deploy && npx prisma db seed && npm start",
    "lint": "eslint .",
    "lint:staged": "lint-staged",
    "migration:generate": "npx migrate dev",
    "migration:run": "npx prisma migrate deploy",
    "dev": "cross-env NODE_ENV=development nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register ./src/server.ts' -e ts",
    "dev:load-envs": "dotenv -e .env.development",
    "dev:migration:generate": "npm run dev:load-envs prisma migrate dev",
    "dev:migration:run": "npm run dev:load-envs prisma migrate deploy",
    "dev:seed": "npm run dev:load-envs prisma db seed",
    "test": "cross-env NODE_ENV=test jest --passWithNoTests --runInBand",
    "test:load-envs": "dotenv -e .env.test",
    "test:migration:generate": "npm run test:load-envs prisma migrate dev",
    "test:migration:run": "npm run test:load-envs prisma migrate deploy",
    "test:seed": "npm run test:load-envs prisma db seed",
    "test:coverage": "cross-env NODE_ENV=test jest --passWithNoTests --runInBand --coverage --collectCoverageFrom='./src/**'",
    "test:watch": "cross-env NODE_ENV=test jest --watch --passWithNoTests --runInBand"
  },
  "keywords": [
    "crud_backend",
    "back-end",
    "typescript",
    "postgres",
    "express",
    "nodejs"
  ],
  "author": "Rafael Oliveira Rosario",
  "license": "ISC",
  "prisma": {
    "seed": "npm run dev:load-envs -- ts-node prisma/seed.ts"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^20.5.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^5.1.0",
    "prettier": "^2.6.2",
    "prisma": "^4.16.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "3.9.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.2.4",
    "typescript-transform-paths": "2.2.3"
  },
  "dependencies": {
    "@brazilian-utils/brazilian-utils": "^1.0.0-rc.12",
    "@prisma/client": "^4.16.0",
    "axios": "^1.1.3",
    "cors": "^2.8.5",
    "dotenv-expand": "^8.0.3",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "moment": "^2.30.1",
    "nodemon": "^3.0.1",
    "reflect-metadata": "^0.1.13",
    "http-status": "^1.5.2",
    "semver": "^7.5.4"
  }
}